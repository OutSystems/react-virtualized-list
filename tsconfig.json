{
    "compilerOptions": {
        "module": "amd",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "outDir": "./output",
		"target": "es5",
        "jsx": "react",
        "experimentalDecorators": true
    },
    "include": ["./src/*"],
    "exclude": ["node_modules"]
}